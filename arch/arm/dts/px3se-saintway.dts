/dts-v1/;

#include "rk3128.dtsi"
#include "rk3128-u-boot.dtsi"
#include <dt-bindings/input/input.h>

/ {
	model = "Saintway PX3SE board";
	compatible = "px3se-saintway";

	chosen {
		stdout-path = &uart2;
	};

       adc-keys {
               compatible = "adc-keys";
               io-channels = <&saradc 1>;

               volup-key {
                       linux,code = <KEY_VOLUMEUP>;
                       label = "volume up";
                       press-threshold-microvolt = <1000>;
               };
       };

	vcc50_otg: vcc50-otg {
		compatible = "regulator-fixed";
		regulator-name = "vcc50_otg";
		gpio = <&gpio0 12 GPIO_ACTIVE_HIGH>;
		pinctrl-names = "default";
		pinctrl-0 = <&otg_drv>;
		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
	};
};

&emmc {
	fifo-mode;
	status = "okay";
};

&u2phy {
	status = "okay";
};

&u2phy_otg {
	status = "okay";
};

&u2phy_host {
	status = "okay";
};

&uart1 {
	clock-frequency = <24000000>;
	u-boot,dm-pre-reloc;
	status = "okay";
};

&uart2 {
	clock-frequency = <24000000>;
	u-boot,dm-pre-reloc;
	status = "okay";
};


&usb_host_ehci {
	status = "okay";
};

&usb_host_ohci {
	status = "okay";
};

&usb_otg {
	vbus-supply = <&vcc50_otg>;
	status = "okay";
};

&saradc {
       status = "okay";
};

&pinctrl {
	usb_otg {
		otg_drv: otg-drv {
			rockchip,pins = <2 12 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};
};
