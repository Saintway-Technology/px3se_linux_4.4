                        <0xef   KEY_LEFT>,
                        <0xee   KEY_RIGHT>,
                        <0xbd   KEY_HOME>,
                        <0xea   KEY_VOLUMEUP>,
                        <0xe3   KEY_VOLUMEDOWN>,
                        <0xe2   KEY_SEARCH>,
                        <0xb2   KEY_POWER>,
                        <0xbc   KEY_MUTE>,
                        <0xec   KEY_MENU>,
                        <0xbf   0x190>,
                        <0xe0   0x191>,
                        <0xe1   0x192>,
                        <0xe9   183>,
                        <0xe6   248>,
                        <0xe8   185>,
                        <0xe7   186>,
                        <0xf0   388>,
                        <0xbe   0x175>;
        };
        ir_key2{
                rockchip,usercode = <0xff00>;
                rockchip,key_table =
                        <0xf9   KEY_HOME>,
                        <0xbf   KEY_BACK>,
                        <0xfb   KEY_MENU>,
                        <0xaa   KEY_REPLY>,
                        <0xb9   KEY_UP>,
                        <0xe9   KEY_DOWN>,
                        <0xb8   KEY_LEFT>,
                        <0xea   KEY_RIGHT>,
                        <0xeb   KEY_VOLUMEDOWN>,
                        <0xef   KEY_VOLUMEUP>,
                        <0xf7   KEY_MUTE>,
                        <0xe7   KEY_POWER>,
                        <0xfc   KEY_POWER>,
                        <0xa9   KEY_VOLUMEDOWN>,
                        <0xa8   KEY_VOLUMEDOWN>,
                        <0xe0   KEY_VOLUMEDOWN>,
                        <0xa5   KEY_VOLUMEDOWN>,
                        <0xab   183>,
                        <0xb7   388>,
                        <0xf8   184>,
                        <0xaf   185>,
                        <0xed   KEY_VOLUMEDOWN>,
                        <0xee   186>,
                        <0xb3   KEY_VOLUMEDOWN>,
                        <0xf1   KEY_VOLUMEDOWN>,
                        <0xf2   KEY_VOLUMEDOWN>,
                        <0xf3   KEY_SEARCH>,
                        <0xb4   KEY_VOLUMEDOWN>,
                        <0xbe   KEY_SEARCH>;
        };
        ir_key3{
                rockchip,usercode = <0x1dcc>;
                rockchip,key_table =
                        <0xee   KEY_REPLY>,
                        <0xf0   KEY_BACK>,
                        <0xf8   KEY_UP>,
                        <0xbb   KEY_DOWN>,
                        <0xef   KEY_LEFT>,
                        <0xed   KEY_RIGHT>,
                        <0xfc   KEY_HOME>,
                        <0xf1   KEY_VOLUMEUP>,
                        <0xfd   KEY_VOLUMEDOWN>,
                        <0xb7   KEY_SEARCH>,
                        <0xff   KEY_POWER>,
                        <0xf3   KEY_MUTE>,
                        <0xbf   KEY_MENU>,
                        <0xf9   0x191>,
                        <0xf5   0x192>,
                        <0xb3   388>,
                        <0xbe   KEY_1>,
                        <0xba   KEY_2>,
                        <0xb2   KEY_3>,
                        <0xbd   KEY_4>,
                        <0xf9   KEY_5>,
                        <0xb1   KEY_6>,
                        <0xfc   KEY_7>,
                        <0xf8   KEY_8>,
                        <0xb0   KEY_9>,
                        <0xb6   KEY_0>,
                        <0xb5   KEY_BACKSPACE>;
        };
};


&pinctrl {
	camera {
		cif_int: cif-int {
			rockchip,pins = <2 2 RK_FUNC_GPIO &pcfg_pull_default>;
		};
	};

	pmic {
		pmic_int_l: pmic-int-l {
			rockchip,pins = <1 12 RK_FUNC_GPIO &pcfg_pull_default>;
		};
	};

	sdmmc {
		sdmmc_pwr: sdmmc-pwr {
			rockchip,pins = <1 14 RK_FUNC_GPIO &pcfg_pull_default>;
		};
	};

	gps {
		gps_pwr: gps-pwr {
			rockchip,pins = <2 25 RK_FUNC_GPIO &pcfg_pull_default>;
		};
	};

	gprs {
		gprs_pwr: gprs-pwr {
			rockchip,pins = <2 8 RK_FUNC_GPIO &pcfg_pull_default>;
		};
	};

	cif {
		cif_pwr: cif-pwr {
			rockchip,pins = <2 22 RK_FUNC_GPIO &pcfg_pull_default>;
		};
	};

	lcdc {
		lcdc_pwr_en: lcdc-pwr-en {
			rockchip,pins = <0 27 RK_FUNC_GPIO &pcfg_pull_default>;
		};
	};

	wifi {
		wifi_en: wifi-en {
			rockchip,pins = <1 3 RK_FUNC_GPIO &pcfg_output_high>;
		};
		wifi_pwr: wifi-pwr {
			rockchip,pins = <3 27 RK_FUNC_GPIO &pcfg_pull_default>;
		};
	};

        wireless-bluetooth {
                uart0_gpios: uart0-gpios {
                        rockchip,pins = <0 17 RK_FUNC_GPIO &pcfg_pull_none>;
                };
        };
	
	4g {
		gprs_slm730: gprs-slm730{
			rockchip,pins = <2 23 RK_FUNC_GPIO &pcfg_pull_default>,
					<2 15 RK_FUNC_GPIO &pcfg_pull_default>,
					<2 14 RK_FUNC_GPIO &pcfg_pull_default>,
					<2 24 RK_FUNC_GPIO &pcfg_pull_default>;
		};
	};

	lcdc {
                lcdc_lcdc: lcdc-lcdc {
                        rockchip,pins =
                                /* depend on the hardware */
                                <2 RK_PB0 1 &pcfg_pull_none>, /* DCLK */
                                <2 RK_PB1 1 &pcfg_pull_none>, /* HSYNC */
                                <2 RK_PB2 1 &pcfg_pull_none>, /* VSYNC */
                                <2 RK_PB3 1 &pcfg_pull_none>, /* DEN */
                                <2 RK_PB4 1 &pcfg_pull_none>, /* DATA10 */
                                <2 RK_PB5 1 &pcfg_pull_none>, /* DATA11 */
                                <2 RK_PB6 1 &pcfg_pull_none>, /* DATA12 */
                                <2 RK_PB7 1 &pcfg_pull_none>, /* DATA13 */
                                <2 RK_PC0 1 &pcfg_pull_none>, /* DATA14 */
                                <2 RK_PC1 1 &pcfg_pull_none>, /* DATA15 */
                                <2 RK_PC2 1 &pcfg_pull_none>, /* DATA16 */
                                <2 RK_PC3 1 &pcfg_pull_none>; /* DATA17 */
                                /* <2 RK_PC4 1 &pcfg_pull_none>, /* DATA18 */
                                /* <2 RK_PC5 1 &pcfg_pull_none>, /* DATA19 */
                                /* <2 RK_PC6 1 &pcfg_pull_none>, /* DATA20 */
                                /* <2 RK_PC7 1 &pcfg_pull_none>, /* DATA21 */
                                /* <2 RK_PD0 1 &pcfg_pull_none>, /* DATA22 */
                                /* <2 RK_PD1 1 &pcfg_pull_none>; /* DATA23 */
                };
        };
};
